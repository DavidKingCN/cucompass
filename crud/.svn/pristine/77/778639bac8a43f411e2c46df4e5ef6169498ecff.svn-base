.header_title{color:#666666;font-size:16px;width:100%;height:60px;line-height:60px;}
.header_title a{color:#666666;}
.nav_btn .active{background:#31A7DA;color:white;}
.nav{position:relative;margin-bottom: 20px;}
.nav p{margin-bottom: 20px;}
th,td{width:50px;height:66px;text-align: center;color:#878787;text-align: center;border:1px solid #DADADA;padding:0px 4px}
td span{display:inline-block;width:20px;height:20px;border:1px solid #DADADA;border-radius:50%;vertical-align: middle;line-height: 20px;margin-right: 10px;}
.foot a{display:inline-block;width:100px;height:30px;background:#EEEEEE;text-align: center;border-radius: 5px;line-height: 30px;margin-left:40px;margin-top: 70px;;}
.foot{width:300px;margin:0px auto;}
.foot .active{background:#31A7DA;color:white;}
a{cursor:pointer}